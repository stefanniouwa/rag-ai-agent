LOG_LEVEL=INFO

# Optional: Application Configuration
MAX_FILE_SIZE_MB=50
MAX_FILES_PER_UPLOAD=10
CHUNK_SIZE=512
CHUNK_OVERLAP=50
TOP_K_RESULTS=4
CHAT_MEMORY_TURNS=5

# Optional: Performance Configuration
